            <font>
                <Font size="14.0"/>
            </font>
            <graphic>
                <FontAwesomeIcon fill="WHITE" glyphName="CLOSE" size="1.3em" wrappingWidth="21.600000381469727"/>
            </graphic>
        </JFXButton>
        <FontAwesomeIcon fill="#286090" glyphName="UNIVERSITY" layoutX="16.0" layoutY="32.0" size="1.8em"/>
    </children>
</AnchorPane>
