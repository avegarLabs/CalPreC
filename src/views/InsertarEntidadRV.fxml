            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <JFXTextField fx:id="field_codigo" labelFloat="true" layoutX="14.0" layoutY="71.0"
                      onAction="#handleDefineEmpresa" prefHeight="30.0" prefWidth="182.0" promptText="Código"/>
        <JFXTextArea fx:id="text_descripcion" labelFloat="true" layoutX="16.0" layoutY="139.0" prefHeight="53.0"
