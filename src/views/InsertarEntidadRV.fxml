<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="263.0" prefWidth="453.0" style="-fx-background-color: #fff;"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="views.InsertarEntidadRVController">
    <children>
        <JFXToolbar layoutX="11.0" layoutY="45.0" prefHeight="10.0" prefWidth="422.0"
