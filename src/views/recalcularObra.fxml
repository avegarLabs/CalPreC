<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="334.0" maxWidth="357.0" minHeight="142.0" minWidth="313.0"
            prefHeight="295.0" prefWidth="351.0" style="-fx-background-color: #ffff;"
            xmlns="http://javafx.com/javafx/10.0.1" fx:controller="views.RecalculraController">
    <children>
        <JFXButton fx:id="btnAction" layoutX="83.0" layoutY="254.0" onAction="#handleCalcular" prefHeight="25.0"
                   prefWidth="190.0" style="-fx-background-color: #286090;" text="Calcular" textFill="WHITE">
            <graphic>
                <FontAwesomeIcon fill="WHITE" glyphName="CALCULATOR"/>
            </graphic>
        </JFXButton>
        <JFXToolbar layoutX="7.0" layoutY="35.0" prefHeight="6.0" prefWidth="339.0"
                    style="-fx-background-color: #286090;"/>
        <Label fx:id="labelText" layoutX="45.0" layoutY="5.0" prefHeight="20.0" prefWidth="262.0" text="Recalcular Obra"
               textFill="#286090">
            <font>
                <Font name="System Bold" size="17.0"/>
            </font>
        </Label>
        <FontAwesomeIcon fill="#286090" glyphName="REFRESH" layoutX="16.0" layoutY="24.0" size="1.5em"/>
        <JFXComboBox fx:id="listEspecialidades" labelFloat="true" layoutX="6.0" layoutY="207.0" prefHeight="25.0"
                     prefWidth="339.0" promptText="Especialidad"/>
        <JFXComboBox fx:id="empresasList" labelFloat="true" layoutX="7.0" layoutY="59.0" prefHeight="25.0"
                     prefWidth="339.0" promptText="Empresas"/>
        <JFXComboBox fx:id="zonaslist" labelFloat="true" onAction="#handleLLenarObjetos" layoutX="8.0" layoutY="104.0"
                     prefHeight="25.0" prefWidth="339.0" promptText="Zonas"/>
        <JFXComboBox fx:id="objetoList" labelFloat="true" layoutX="6.0" layoutY="155.0" prefHeight="25.0"
                     prefWidth="339.0" promptText="Objetos"/>
    </children>
</AnchorPane>
